{
    "title": "AgendaItem",
    "description": "{{ agendaItem.description }}",
    "type": "object",
    "required": [
        "id",
        "type"
    ],
    "properties": {
        "id": {
            "type": "string",
            "format": "url"
        },
        "type": {
            "type": "string",
            "pattern": "^https\\:\\/\\/schema\\.oparl\\.org\\/1\\.1\\/AgendaItem$"
        },
        "meeting": {
            "description": "{{ agendaItem.meeting.description }}",
            "type": "string",
            "format": "url",
            "references": "Meeting"
        },
        "number": {
            "description": "{{ agendaItem.number.description }}",
            "type": "string"
        },
        "name": {
            "description": "{{ agendaItem.name.description }}",
            "type": "string"
        },
        "public": {
            "description": "{{ agendaItem.public.description }}",
            "type": "boolean"
        },
        "consultation": {
            "description": "{{ agendaItem.consultation.description }}",
            "references": "Consultation",
            "backreference": "agendaItem",
            "cardinality": "1:1",
            "type": "string",
            "format": "url"
        },
        "result": {
            "description": "{{ agendaItem.result.description }}",
            "type": "string"
        },
        "resolutionText": {
            "description": "{{ agendaItem.resolutionText.description }}",
            "type": "string"
        },
        "resolutionFile": {
            "description": "{{ agendaItem.resolutionFile.description }}",
            "type": "object",
            "schema": "File.json"
        },
        "auxiliaryFile": {
            "description": "{{ agendaItem.auxiliaryFile.description }}",
            "type": "array",
            "items": {
                "type": "object",
                "schema": "File.json"
            }
        },
        "start": {
            "description": "{{ agendaItem.start.description }}",
            "type": "string",
            "format": "date-time"
        },
        "end": {
            "description": "{{ agendaItem.end.description }}",
            "type": "string",
            "format": "date-time"
        },
        "license": {
            "type": "string"
        },
        "keyword": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "web": {
            "type": "string",
            "format": "url"
        }
    }
}
